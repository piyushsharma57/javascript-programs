// DOM Manipulation : Document Object Model
let h1 = document.getElementById("heading")
let h1Class = document.getElementsByClassName("headClass")
let h1Tag = document.getElementsByTagName("h1")
let h1Parent = h1Tag[0].parentElement
let h1Sibling = h1Tag[0].nextSibling
let h1ElementSibling = h1Tag[0].nextElementSibling.nextElementSibling
let previousSibling = h1ElementSibling.previousElementSibling;
let childElCount = h1Parent.childElementCount
let childNodes = h1Parent.childNodes
let childrens = h1Parent.children

